    // Activity 1: part 2 Drop function
    function act1Drop(ev) {
        ev.preventDefault();
        var id = ev.dataTransfer.getData("text");
        var target = ev.target;
        var sentence = document.getElementById(id).innerHTML;
        var position = splitString(sentence);

        document.getElementById(id).innerHTML = "";
        target.innerHTML = position;
    }

