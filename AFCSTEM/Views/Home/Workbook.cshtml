    function allowDrop(ev) {
        ev.preventDefault();
    }

    function drag(ev) {
        ev.dataTransfer.setData("text", ev.target.id);
    }

    // Activity 1: part 1 Drop function
    function drop(ev) {
        ev.preventDefault();
        var id = ev.dataTransfer.getData("text");
        var target = ev.target;
        var text = document.getElementById(id).innerHTML;

        document.getElementById(id).innerHTML = "";
        document.getElementById(id).style.visibility = "hidden";
        target.innerHTML = text;
    }

    //Activity 1: part 2 Drop function
    function act1Drop(ev) {
        ev.preventDefault();
        var id = ev.dataTransfer.getData("text");
        var target = ev.target;
        var sentence = document.getElementById(id).innerHTML;
        var position = splitString(sentence);

        document.getElementById(id).innerHTML = "";
        target.innerHTML = position;
    }

    function splitString(sentence) {
        var string = sentence;
        var code = "";
        string = string.split(" ");
        //console.log(string);
        for (i = 0; i < string.length; i++) {
            var word = string[i];
            var subcode = word.charAt(0);
            if (subcode == '(') { } else {
                code = code + subcode;
            }
            //console.log(code);
        }
        return code;
    }




</script>
