<script>


    var coinCount = 0;
    goalCount = 0;
    behindCount = 0;
    missCount = 0;

    jQuery(document).ready(function ($) {
        
        $('#coin, #coin2').on('click', function () {
            coinCount++;
            var flipResult = Math.random();
            var flipResult2 = Math.random();
            $('#coin').removeClass();
            $('#coin2').removeClass();
            setTimeout(function () {
                if (flipResult <= 0.5) {
                    $('#coin').addClass('heads');
                    console.log('coin1 - it is head');
                }
                if (flipResult2 <= 0.5) {
                    $('#coin2').addClass('heads');
                    console.log('coin2 - it is head');
                }
                if(flipResult > 0.5) {
                    $('#coin').addClass('tails');
                    console.log('coin1 - it is tails');
                }

                if (flipResult2 > 0.5) {
                    $('#coin2').addClass('tails');
                    console.log('coin2  - it is tails');
                }
                console.log($("#hh1").text());





                if (($('#coin').attr('class') == "heads") && ($('#coin2').attr('class') == "heads")) {
                    console.log(" in hh ")
                    var id = "hh" + coinCount;
                    $("#" + id).text("Goal");
                    goalCount++;
                }

                else if (($('#coin').attr('class') == "tails") && ($('#coin2').attr('class') == "tails")) {
                    console.log(" in tt " )
                    var id = "tt" + coinCount;
                    $("#" + id).text("Miss");
                    missCount++;
                }

                else {
                    var id = "ht" + coinCount;
                    $("#" + id).text("Behind");
                    behindCount++;

                }

                $("#gc").text(goalCount);
                $("#mc").text(missCount);
                $("#bc").text(behindCount);


                



            }, 100);
        });

      
    });




    function allowDrop(ev) {
        ev.preventDefault();
    }

    function drag(ev) {
        ev.dataTransfer.setData("text", ev.target.id);
    }

    function drop(ev) {
        ev.preventDefault();
        var data = ev.dataTransfer.getData("text");
        var target = ev.target;
        var text = document.getElementById(data).innerHTML;

        document.getElementById(data).innerHTML = "";
        target.innerHTML = text;
    }

    function act1Drop(ev) {
        ev.preventDefault();
        var id = ev.dataTransfer.getData("text");
        var target = ev.target;
        var sentence = document.getElementById(id).innerHTML;
        var position = splitString(sentence);

        target.innerHTML = position;


    }

    function splitString(sentence) {
        var string = sentence;
        var code = "";
        string = string.split(" ");
        //console.log(string);
        for (i = 0; i < string.length; i++) {
            var word = string[i];
            var subcode = word.charAt(0);
            if (subcode == '(') { } else {
                code = code + subcode;
            }
            //console.log(code);
        }
        return code;
    }




</script>
