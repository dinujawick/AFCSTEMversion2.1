    List<Player> forward = new List<Player>();
    List<Player> mid = new List<Player>();
    List<Player> back = new List<Player>();
    List<Player> ruck = new List<Player>();

    @for (int i = 0; i < selectedPlayers.Count; i++)
    {
        Player player = selectedPlayers[i];

        if (player != null)
        {
            if (player.Position == "FWD")
            {
                forward.Add(player);
            }
            else if (player.Position == "MID")
            {
                mid.Add(player);
            }
            else if (player.Position == "RUCK")
            {
                ruck.Add(player);
            }
            else
            {
                back.Add(player);
            }
