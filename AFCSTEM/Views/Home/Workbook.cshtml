    function check(id) {
        var table = document.getElementById(id);
        var isEmpty = 0;
        for (i = 0; i < table.rows.length; i++) {
            var row = table.rows[i];
            for (j = 0; j < row.cells.length; j++) {
                var cell = row.cells[j]
                if (window.getComputedStyle(cell).visibility == "hidden") {
                    // the player table is empty
                    isEmpty = 1;
                } else {
                    // The table is populated i.e there are still players in the table
                    isEmpty = 0;
                    break;
                }
            }
        }
        return isEmpty;
    }


