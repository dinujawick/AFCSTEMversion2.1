    // Method to split positions into the abbreviated versions. i.e Full Forward == FF
    function splitString(sentence) {
        var string = sentence;
        var code = "";
        string = string.split(" ");

        for (i = 0; i < string.length; i++) {
            var word = string[i];
            var subcode = word.charAt(0);
            if (subcode == '(') { } else {
                code = code + subcode;
            }
        }
        return code;
    }

